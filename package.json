{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup && pnpm postinstall",
    "upgrade:nuxt": "nuxt upgrade --force",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "cleanup:nm": "nuxt cleanup && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@floating-ui/vue": "1.0.3",
    "@pinia/nuxt": "0.5.1",
    "@tinymce/tinymce-vue": "5.1.1",
    "@vee-validate/nuxt": "4.12.4",
    "@vee-validate/yup": "4.12.4",
    "pinia": "2.1.7",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@egoist/tailwindcss-icons": "1.7.2",
    "@element-plus/nuxt": "1.0.7",
    "@iconify-json/ep": "1.1.14",
    "@iconify-json/fa6-solid": "1.1.20",
    "@nuxt/devtools": "1.0.8",
    "@nuxtjs/i18n": "8.0.0",
    "@sarast/eslint-config": "0.0.3",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.11.0",
    "@vueuse/core": "10.7.2",
    "@vueuse/nuxt": "10.7.2",
    "autoprefixer": "10.4.16",
    "dotenv": "16.3.1",
    "element-plus": "2.5.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "lint-staged": "15.2.0",
    "nuxt": "^3.9.1",
    "postcss": "8.4.33",
    "prettier": "3.2.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "rimraf": "5.0.5",
    "sass": "1.69.7",
    "simple-git-hooks": "2.9.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vitest": "1.2.0",
    "vue-tsc": "1.8.27"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit \"$1\"",
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,cjs,mjs,ts,cts,mts,json,md,yaml}": "pnpm lint",
    "*": "pnpm format"
  }
}
