{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup && pnpm postinstall",
    "upgrade:nuxt": "nuxt upgrade --force",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "cleanup:nm": "nuxt cleanup && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@floating-ui/vue": "1.0.2",
    "@pinia/nuxt": "0.4.11",
    "@tinymce/tinymce-vue": "5.1.0",
    "@vee-validate/nuxt": "4.10.8",
    "@vee-validate/yup": "4.10.8",
    "pinia": "2.1.6",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.39.8",
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@egoist/tailwindcss-icons": "1.1.0",
    "@element-plus/nuxt": "1.0.5",
    "@iconify-json/ep": "1.1.11",
    "@iconify-json/fa6-solid": "1.1.13",
    "@nuxt/devtools": "0.7.1",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@tailwindcss/forms": "0.5.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "20.4.5",
    "@vueuse/core": "10.2.1",
    "@vueuse/nuxt": "10.2.1",
    "autoprefixer": "10.4.14",
    "dotenv": "16.3.1",
    "element-plus": "2.3.8",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "nuxt": "^3.6.5",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "rimraf": "5.0.1",
    "sass": "1.64.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vitest": "0.33.0",
    "vue-tsc": "1.8.8"
  },
  "lint-staged": {
    "*.{vue,js,cjs,mjs,ts,cts,mts,json,md,yaml}": "pnpm lint",
    "*": "pnpm format"
  }
}
