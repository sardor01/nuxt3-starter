{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup && pnpm postinstall",
    "upgrade:nuxt": "nuxt upgrade --force",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "cleanup:nm": "nuxt cleanup && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.0",
    "@pinia/nuxt": "^0.4.11",
    "@tinymce/tinymce-vue": "^5.1.0",
    "@vee-validate/nuxt": "^4.9.5",
    "@vee-validate/yup": "^4.9.5",
    "pinia": "^2.1.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@egoist/tailwindcss-icons": "^1.0.7",
    "@element-plus/nuxt": "^1.0.5",
    "@iconify-json/ep": "^1.1.10",
    "@iconify-json/fa6-solid": "^1.1.13",
    "@nuxt/devtools": "^0.5.5",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.2.5",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "element-plus": "^2.3.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "*.{vue,js,cjs,mjs,ts,cts,mts,json,md,yaml}": "pnpm lint",
    "*": "pnpm format"
  }
}
