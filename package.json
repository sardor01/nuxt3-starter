{
  "private": true,
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup && pnpm postinstall",
    "upgrade:nuxt": "nuxt upgrade --force",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "cleanup:nm": "nuxt cleanup && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@pinia/nuxt": "^0.4.9",
    "@popperjs/core": "^2.11.7",
    "@tinymce/tinymce-vue": "^5.1.0",
    "@vee-validate/nuxt": "^4.8.6",
    "@vee-validate/yup": "^4.8.6",
    "ant-design-vue": "^3.2.17",
    "pinia": "^2.0.35",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.15.13",
    "@vueuse/core": "^10.0.2",
    "@vueuse/nuxt": "^10.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "nuxt": "^3.4.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.4.2"
  },
  "lint-staged": {
    "*.{vue,js,cjs,mjs,ts,cts,mts,json,md,yaml}": "pnpm lint",
    "*": "pnpm format"
  }
}
