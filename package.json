{
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup && pnpm postinstall",
    "upgrade:nuxt": "nuxt upgrade --force",
    "test": "vitest --config vitest.config.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lf": "pnpm lint:fix && pnpm format:fix",
    "cleanup:nm": "nuxt cleanup && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@pinia/nuxt": "^0.4.10",
    "@popperjs/core": "^2.11.7",
    "@tinymce/tinymce-vue": "^5.1.0",
    "@vee-validate/nuxt": "^4.9.2",
    "@vee-validate/yup": "^4.9.2",
    "pinia": "^2.0.36",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@element-plus/icons-vue": "^2.1.0",
    "@element-plus/nuxt": "^1.0.4",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.1.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "element-plus": "^2.3.4",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.4.3",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  },
  "lint-staged": {
    "*.{vue,js,cjs,mjs,ts,cts,mts,json,md,yaml}": "pnpm lint",
    "*": "pnpm format"
  }
}
